.\" Manpage for <%= resume['basics']['name'] %>
.\" Contact <%= resume['basics']['email'] %> for more info
.TH ian.bissett 7 "<%= date.strftime("%d %b %Y") %>" "<%= resume['meta']['version'] %>" "<%= resume['basics']['name'] %>"
.SH NAME
<%= resume['basics']['name'] %> \- <%= resume['basics']['label'] %>
.SH SYNOPSIS
<%= resume['basics']['summary'] %>
.SH DETAILS

.TS
tab (@);
l lx.
\fBemail: \fR@T{
<%= resume['basics']['email'] %>
T}
\fBPhone: \fR@T{
<%= resume['basics']['phone'] %>
T}
\fBLocation: \fR@T{
<%= resume['basics']['location']['address']%>
T}
\fBURL: \fR@T{
<%= resume['basics']['url']%>
T}
<% if resume['basics']['profiles'] -%>
<% resume['basics']['profiles'].each do |pr| -%>
\fB<%= pr['network'] %>: \fR@T{
<%= pr['url']%>
T}
<% end -%>
<% end -%>
.TE

.TE
.SH EXPERIENCE
<% resume['work'].each do |job| -%>
.SS <%= job['name'] %> \- <%= job['position'] %> \- <%= sdate(job['startDate']) %> \- <%= sdate(job['endDate']) %>
<%= job['location'] %> \- 
.I <%= job['url'] %>

<% if job['summary'] -%>
.P
<%= job['summary'] %>
<% end -%>
<% if job['highlights'] -%>
<% job['highlights'].each do |hl| -%>
.IP \[bu]
<%= hl %>
<% end -%>
<% end -%>
<% end -%>
.SH TRAINING \& CERTIFICATION
<% resume['certificates'].each do |cert| -%>
.IP \[bu]
<%= cert['name'] %> - <%= cert['issuer'] %> - <%= sdate(cert['startDate']) %>
<% end -%>
<% if additional['interests'] -%>
.SH HOBBIES & INTERESTS
<% if additional['interests']['label'] -%>
.SS <%= additional['interests']['label'] %>
<% end -%>
.P
<%= additional['interests']['summary'] %>
<% end %>
<% if additional['additional'] -%>
.SH FURTHER INFO
.P
<%= additional['additional']['summary'] %>
<% end %>
